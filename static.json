{
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=512000"
    },
    "/assets/webfonts/*": {
      "Access-Control-Allow-Origin": "*"
    }
  },
  "https_only": true,
  "proxies": {
    "/api/": {
      "origin": "https://scheduling-server.herokuapp.com/api/rooms"
    }
  },
  "root": ".",
  "routes": {
    "/**": "index.html"
  }
}
